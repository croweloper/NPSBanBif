<script type="text/javascript">
        var APPurl = "@ViewBag.APPURL";

        var canal = document.getElementById("hidCanal").value;
        console.log(canal);

        var dni = localStorage.dnilogin;
        var documento = localStorage.dnilogin;
        var controlador = localStorage.nuevoControlador;
        var oficina = localStorage.codoficina;
        var usu = document.getElementById("hidIdUsuario").value;

        

        function CargarPagina() {
            var flag = document.getElementById("hidLoad").value;
            var rate = document.getElementById("hidRating").value;
            var available = document.getElementById("hidAvailable").value;
            var mensaje = document.getElementById("hidMensaje").value;
            var pregunta = document.getElementById("hidPregunta").value;

            localStorage.setItem('MensajePoll', mensaje);

            if (flag == "0") {
                window.location.href = appurl + "/gracias/index";
            }

            if (available == "2" || available == "3") {
                window.location.href = APPurl + "/Gracias/Index";
            }

            document.getElementById("divPregunta").innerHTML = "1. " +pregunta;

            Rating(rate);


            /*MATRIZ CANALES*/
            var pregunta2 = "2. ";
            var pregunta3 = "<br> 3. ";
            if (document.getElementById("hidCanal").value == "BPI") {
                pregunta2 += "Por favor, califique ¿Qué tan satisfecho se encuentra con relación a …?";
                pregunta3 += "En los últimos dos meses ¿Ha tenido o se ha presentado algún problema mientras usaba nuestra Banca por Internet?";

                $('#divPregunta3').html(pregunta3);
                $('#divCanalBPI').show();
                $('#divPregunta3').show();
                $('#divCampoPregunta3').show();
            } else if (document.getElementById("hidCanal").value == "APP") {
                pregunta2 += "Por favor, califique ¿Qué tan satisfecho se encuentra con relación a …?";
                pregunta3 += "En los últimos dos meses ¿Ha tenido o se ha presentado algún problema mientras usaba nuestra APP BanBif?";

                $('#divPregunta3').html(pregunta3);
                $('#divCanalAPP').show();
                $('#divPregunta3').show();
                $('#divCampoPregunta3').show();
            } else {
                pregunta2 += "Por favor, califique ¿Qué tan satisfecho se encuentra con relación a …?";
                $('#divCanalBT').show();
            }


            $('#divPregunta2').html(pregunta2);

        }
        function Rating(rate) {
            for (i = 0; i <= 10; i++) {
                document.getElementById("arrow" + i).style.visibility = "hidden";
                if (rate == i) {
                    document.getElementById("arrow" + i).style.visibility = "visible";
                    document.getElementById("hidRating").value = rate;
                }
            }

            if (rate <= 10) {
                document.getElementById("pPreguntaComentario").innerHTML = "¿Qué fue lo que más le gustó?"; //"¿Qué es lo que más le gusta de BANBIF?";
            }
            if (rate <= 8) {
                document.getElementById("pPreguntaComentario").innerHTML = "¿Qué cambiaría para mejorar?";// "¿Qué nos faltaría para mejorar?";
            }
            if (rate <= 6) {
                document.getElementById("pPreguntaComentario").innerHTML = "¿Qué fue lo que menos le gustó?";//"¿Por qué motivo nos brinda esa calificación?";
            }
        }
        function RegistrarVoto() {
            var rate = document.getElementById("hidRating").value;
            var comentario = document.getElementById("comments").value;
            var idUsuario = dni;

            if (comentario == "") {
                alert("Por favor escribir un comentario.");
                return false;
            }


            var oBE = {};
            oBE.id = idUsuario;
            oBE.result = rate;
            oBE.comments = comentario;
            oBE.token = "";

            oBE.documento = idUsuario;
            oBE.canal = controlador;
            oBE.oficina = oficina;
            oBE.VOTO = rate
            oBE.comentario_VOTO = comentario
            oBE.COMENTARIO_PREG2 = ""
            oBE.COMENTARIO_PREG3 = document.getElementById("txtPregunta3").value + " | " + document.getElementById("hidCanal").value;

            


            var Opciones = [];
